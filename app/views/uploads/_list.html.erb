<h1>Listing Uploads</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Key</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% uploads.each do |upload| %>
      <tr>
        <td><%= linkt_to upload.nam, upload_path(upload) %></td>
        <td><%= s3_link Tenant.current_id, upload.key %></td>
        <td><%= link_to 'Destroy', upload, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Upload', new_upload_path(project_id: @project.id) %>